{
  "compilerOptions": {
    "target": "es2015",
    "noImplicitAny": false,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "outDir": ".tempOutDir"
  },
  "exclude": [
    "node_modules"
  ]
}