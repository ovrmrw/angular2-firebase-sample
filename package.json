    "@types/node": "^4.0.28-alpha",
    "@types/node-uuid": "0.0.26-alpha",
    "@types/power-assert": "0.0.26-alpha",
    "@types/power-assert-formatter": "0.0.25-alpha",
    "awesome-typescript-loader": "^0.19.0",
    "babel-plugin-espower": "^2.2.0",
