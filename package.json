    "del": "^2.2.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-ignore": "^2.0.1",
    "gulp-mocha": "^2.2.0",
    "gulp-nightwatch": "^0.2.9",
    "gulp-plumber": "^1.1.0",
    "gulp-typescript": "^2.13.6",
    "html-loader": "^0.4.3",
    "jasmine": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
